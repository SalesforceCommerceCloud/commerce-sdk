    ): Promise<Response | {{addNamespace (getReturnTypeFromOperation this) @root.name.upperCamelCase}}> {
    
      const parameters = (options && options.parameters) ? options.parameters : {};
      
      const pathParameters = {
      {{#each ../parameters}}
        "{{{name}}}": parameters["{{{name}}}"]
