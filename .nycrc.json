{
  "all": true,
  "branches": 80,
  "check-coverage": true,
  "exclude": [
    "src/updateApis.ts",
    "src/lib/config.ts",
    "src/generate-oas.ts",
    "src/generateVersionTable.ts",
    "src/removeInternalOas.ts",
    "src/static/helpers/index.ts",
    "src/lib/utils.ts"
  ],
  "extends": "@istanbuljs/nyc-config-typescript",
  "extension": [
    ".ts"
  ],
  "functions": 80,
  "include": [
    "src/**/*.ts"
  ],
  "lines": 80,
  "per-file": true,
  "reporter": [
    "lcov",
    "text"
  ],
  "statements": -10
}
