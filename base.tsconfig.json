{
    "compilerOptions": {
        "module": "commonjs",
        "allowSyntheticDefaultImports": true,
        "alwaysStrict": true,
        "declaration": true,
        "sourceMap": true,
        "target": "ES6",
        "esModuleInterop": true,
        "declarationMap": true,
        "forceConsistentCasingInFileNames":true,
        "importHelpers": true,
        "incremental": false,
        "moduleResolution": "Node",
        "noEmitOnError": true,
        "noErrorTruncation": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "preserveConstEnums": true
    }
}